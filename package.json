{
  "name": "mdimg",
  "version": "0.0.1",
  "description": "Covert Markdown or HTML to image",
  "keywords": [
    "markdown",
    "html",
    "image"
  ],
  "main": "lib/mdimg.js",
  "module": "lib/mdimg.mjs",
  "bin": {
    "mdimg": "bin/mdimg.js"
  },
  "files": [
    "bin/",
    "lib/",
    "src/"
  ],
  "scripts": {
    "mdimg": "babel-node ./bin/mdimg.js",
    "test": "babel-node ./test/index.js",
    "build": "npm run rollup",
    "rollup": "rollup -c rollup.config.js",
    "lint": "prettier --write **/* --ignore-path .eslintignore --ignore-unknown"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LolipopJ/mdimg.git"
  },
  "author": "LolipopJ",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LolipopJ/mdimg/issues"
  },
  "homepage": "https://github.com/LolipopJ/mdimg#readme",
  "dependencies": {
    "cheerio": "^1.0.0-rc.10",
    "commander": "^9.0.0",
    "marked": "^4.0.12",
    "puppeteer": "^13.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.4",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/node": "^7.16.8",
    "@babel/preset-env": "^7.16.11",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.5.1",
    "rollup": "^2.67.2",
    "rollup-plugin-license": "^2.6.1",
    "rollup-plugin-scss": "3",
    "sass": "^1.49.7"
  }
}
